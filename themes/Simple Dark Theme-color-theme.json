{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.border": "#e9bf0421",
    "activityBarBadge.background": "#007acc",
    "editor.background": "#151516",
    "editor.foreground": "#d4d4d4",
    "editor.inactiveSelectionBackground": "#3a3d41",
    "editor.lineHighlightBackground": "#000000",
    "editor.selectionBackground": "#394f689a",
    "editor.selectionHighlightBackground": "#394f689a",
    "editorCursor.background": "#ffffff",
    "editorIndentGuide.activeBackground": "#707070",
    "editorIndentGuide.background": "#404040",
    "input.placeholderForeground": "#a6a6a6",
    "list.dropBackground": "#383b3d",
    "panel.background": "#08090f",
    "panel.border": "#e9bf0421",
    "settings.numberInputBackground": "#292929",
    "settings.textInputBackground": "#292929",
    "sideBar.background": "#08090f",
    "sideBar.border": "#ffd90044",
    "sideBar.foreground": "#ececec",
    "sideBarSectionHeader.foreground": "#8d8d8d",
    "sideBarTitle.foreground": "#bbbbbb",
    "statusBar.background": "#0e0d03",
    "tab.activeBackground": "#e9bf0421",
    "titleBar.border": "#ffd900"
  },
  "tokenColors": [
    // ---------------------------------------------------------------------------------------------
    // -------------------------            VS CODE DEFAULT THEME            -----------------------
    // ---------------------------------------------------------------------------------------------
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#646695"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.attribute.scss",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "beginning.punctuation.definition.quote.markdown",
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#d16969"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Regular expression groups",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "keyword.operator.negation.regexp",
        "support.other.parenthesis.regexp"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": [
        "constant.character.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.other.character-class.regexp",
        "constant.character.set.regexp"
      ],
      "settings": {
        "foreground": "#d16969"
      }
    },
    {
      "scope": ["keyword.operator.or.regexp", "keyword.control.anchor.regexp"],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": "keyword.operator.quantifier.regexp",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    // ---------------------------------------------------------------------------------------------
    // -------------------------            NORMAL TEXT            ---------------------------------
    // ---------------------------------------------------------------------------------------------
    {
      "name": "NORMAL TEXT",
      "scope": [
        "beginning.punctuation.definition.list.markdown", // md list item
        "entity.name.function",
        "entity.name.tag",
        "entity.name.variable",
        "meta.object-literal.key.js", // js object keys... needed?
        "punctuation.definition.parameters.begin", // opening bracket
        "punctuation.definition.parameters.end", // closing bracket
        "punctuation.section.embedded", // js: {} in React component
        "source.coffee.embedded",
        "source", // default text
        "support.function",
        "support.type.property-name",
        "support.type.property-name", // json property
        "support.type.vendored.property-name",
        "support.type.vendored.property-name", // css no special treatement to certain css properties
        "support.variable",
        "variable.css",
        "variable.other.less",
        "variable.parameter",
        "variable.scss",
        "variable"
      ],
      "settings": {
        "foreground": "#e4e4e4",
        "fontStyle": ""
      }
    },
    {
      "name": "NORMAL TEXT (bold)",
      "scope": [
        "markup.bold",
        "variable.parameter.function",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "bold"
      }
    },
    // ---------------------------------------------------------------------------------------------
    // -------------------------            KEYWORDS            ------------------------------------
    // ---------------------------------------------------------------------------------------------
    {
      "name": "KEYWORDS (italic)",
      "scope": [
        "constant.language", // true false
        "keyword.operator.cast",
        "keyword.operator.expression",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.new",
        "keyword.operator",
        "keyword.operator.sizeof",
        "keyword.control",
        "storage",
        "keyword",
        "punctuation.accessor",
        "punctuation.separator",
        "storage.modifier", // java public protected and private
        "storage.type", // const
        "support.function.builtin.go", // go make() len()...
        "variable.language.this", // java this
        "variable.language.special.self.python", // python self
        "variable.parameter.function.language.special.self.python" // python self as parameter
      ],
      "settings": {
        "foreground": "#8d8d8d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "KEYWORDS (no italic)",
      "scope": [
        "keyword.operator", // + =
        "punctuation.definition",
        "punctuation.section.embedded.begin.php", // php <?
        "punctuation.section.embedded.end.php",
        "storage.type.function.arrow", // ->
        "support.function.magic.python" // python __init__
      ],
      "settings": {
        "foreground": "#8d8d8d",
        "fontStyle": ""
      }
    },
    // ---------------------------------------------------------------------------------------------
    // -------------------------            CLASSES            -------------------------------------
    // ---------------------------------------------------------------------------------------------
    {
      "name": "CLASSES",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.other.inherited-class", // java classes ...
        "keyword.channel.go", // go chan type
        "keyword.map.go", // go map type
        "markup.fenced_code.block", // md code block
        "markup.inline.raw", // md: code inline
        "meta.return-type",
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "storage.type.annotation.groovy",
        "storage.type.annotation.java", // java annotations
        "storage.type.boolean.go", // go bool type
        "storage.type.byte.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.generic.groovy",
        "storage.type.generic.java", // java classes ...
        "storage.type.groovy",
        "storage.type.java", // java classes
        "storage.type.modifier.cs",
        "storage.type.numeric.go", // go numeric type
        "storage.type.object.array.groovy",
        "storage.type.object.array.java", // java classes ...
        "storage.type.parameters.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.array.java",
        "storage.type.primitive.groovy",
        "storage.type.primitive.java",
        "storage.type.string.go", // go string type
        "storage.type.token.java",
        "storage.type.uintptr.go",
        "storage.type.variable.cs",
        "support.class",
        "support.constant.dom",
        "support.constant.json", // js: JSON as class
        "support.constant.math",
        "support.type"
      ],
      "settings": {
        "foreground": "#ffb1fb",
        "fontStyle": ""
      }
    },
    {
      "name": "CLASSES (darker)",
      "scope": [
        "entity.other.attribute-name.pseudo-element.css", // css sub class
        "entity.other.attribute-name" // js: attributes for creating a React component
      ],
      "settings": {
        "foreground": "#cea8ce",
        "fontStyle": ""
      }
    },
    // ---------------------------------------------------------------------------------------------
    // -------------------------            FUNCTIONS            -----------------------------------
    // ---------------------------------------------------------------------------------------------
    {
      "name": "FUNCTIONS",
      "scope": [
        "support.function", // function call
        "support.constant.handlebars", // function declaration?
        "entity.name.function", // function declaration
        "meta.function-call.generic.python", // python function call
        "entity.name.tag.css" // css class as a function
      ],
      "settings": {
        "foreground": "#a7dff5",
        "fontStyle": ""
      }
    },
    {
      "name": "FUNCTIONS (bold)",
      "scope": [
        "markup.heading", // md headers
        "markup.underline" // md links
      ],
      "settings": {
        "foreground": "#a7dff5",
        "fontStyle": "bold"
      }
    },
    // ---------------------------------------------------------------------------------------------
    // -------------------------            STRINGS & VALUES            ----------------------------
    // ---------------------------------------------------------------------------------------------
    {
      "name": "STRINGS",
      "scope": [
        "constant.character.escape",
        "constant.other.set.regexp", // python surronding [] in regex
        "punctuation.definition.string",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded", // ?
        "string",
        "support.constant.property-value" // css value
      ],
      "settings": {
        "foreground": "#d7ba7d",
        "fontStyle": ""
      }
    },
    {
      "name": "STRINGS (bold)",
      "scope": ["constant.other.placeholder"],
      "settings": {
        "foreground": "#d7ba7d",
        "fontStyle": "bold"
      }
    },
    {
      "name": "STRINGS (darker)",
      "scope": [
        "constant.character.set.regexp", // regex
        "constant.character", // constants
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#ffbf33",
        "fontStyle": ""
      }
    },
    // ---------------------------------------------------------------------------------------------
    // -------------------------            COMMENTS            ------------------------------------
    // ---------------------------------------------------------------------------------------------
    {
      "name": "COMMENTS",
      "scope": [
        "punctuation.definition.comment", // comment slash
        "comment.block.documentation",
        "comment"
      ],
      "settings": {
        "foreground": "#688d6e",
        "fontStyle": ""
      }
    },
    {
      "name": "COMMENTS (bold)",
      "scope": [
        "keyword.other.phpdoc" // php doc: keywords
      ],
      "settings": {
        "foreground": "#688d6e",
        "fontStyle": "bold"
      }
    }
  ]
}
